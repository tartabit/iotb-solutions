{
  "templateDef": [
    {
      "name": "Generic Event",
      "key": "generic_event",
      "desc": "Generic event to watch to send data..",
      "type": "generic-key"
    },
    {
      "name": "HTTP Server",
      "key": "server",
      "desc": "IoT Bridge will POST to this URL.",
      "value": "https://<address>"
    }
  ],
  "services": [
    {
      "id": "6727c5c1daab4af297728a84",
      "name": "HTTP Publisher",
      "key": "http-sender",
      "modelId": "81da3cb1d06f8a8fbbcbefad",
      "params": {
        "endpoint_required": "false"
      }
    }
  ],
  "triggers": [
    {
      "id": "6727c63fff0b7e01308b1d82",
      "name": "{{.generic_event}} - POST to HTTP Server",
      "type": "script",
      "def": "if(event.endpoint) {\r\n  http.post('http-sender','{{.server}}' + '/' + event.endpoint.key,event.data.data)\r\n} else {\r\n  http.post('http-sender','{{.server}}',event.data.data)\r\n}",
      "filterType": "generic",
      "filterGeneric": "{{.generic_event}}"
    }
  ]
}